{
  "expo": {
    "name": "code-leap-test",
    "slug": "code-leap-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_codeleap.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/codeleap_logo_black.png",
      "resizeMode": "center",
      "backgroundColor": "#FFFFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
